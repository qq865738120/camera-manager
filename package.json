        "@rematch/loading": "^1.2.1",
        "@types/react-beautiful-dnd": "^12.1.2",
        "antd": "^4.12.3",
        "axios": "^0.21.0",
        "classnames": "^2.2.6",
        "connected-react-router": "^6.8.0",
        "events": "^3.2.0",
