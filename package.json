        "friendly-errors-webpack-plugin": "^1.7.0",
        "html-webpack-externals-plugin": "^3.8.0",
        "html-webpack-plugin": "^5.0.0-alpha.6",
        "husky": "^4.2.3",
        "less": "^3.12.2",
        "less-loader": "^7.1.0",
        "lint-staged": "^10.0.8",
        "mini-css-extract-plugin": "^1.3.1",
