{
    "name": "react-ts-project-template",
    "version": "1.0.0",
    "description": "react + ts 项目模板",
    "main": "index.js",
    "keywords": [],
    "author": "yjd",
    "license": "ISC",
    "scripts": {
        "dll": "webpack --config build/webpack.dll.config.js",
        "dev": "cross-env NODE_ENV='development' webpack-cli serve --config build/webpack.dev.config.js --color",
        "build": "cross-env NODE_ENV='production' webpack --config build/webpack.prod.config.js --progress --color",
        "buildSM": "cross-env NODE_ENV='production' --SPEED_MEASUREMENT=true  webpack --config build/webpack.prod.config.js --progress --color",
        "type-check": "tsc --watch",
        "nodemon": "nodemon --verbose  --ignore  /src/  -e  js   --exec \"cross-env NODE_ENV='development' webpack-cli serve --config build/webpack.dev.config.js --color\"",
        "nc-u": "cross-env NPM_CHECK_INSTALLER=cnpm npm-check -u",
        "analyz": "webpack-bundle-analyzer --port 8888 ./dist/stats.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-decorators": "^7.12.1",
